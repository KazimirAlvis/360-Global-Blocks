{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "global360blocks/symptoms-ai",
	"version": "1.0.0",
	"title": "AI Symptoms Information",
	"category": "common",
	"icon": "admin-page",
	"description": "Generate comprehensive symptom information using AI. Just enter a symptom and get detailed medical information.",
	"keywords": ["symptoms", "medical", "ai", "health", "information"],
	"textdomain": "360-global-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"supports": {
		"html": false,
		"align": true,
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"attributes": {
		"symptom": {
			"type": "string",
			"default": ""
		},
		"ai_content": {
			"type": "string",
			"default": ""
		},
		"is_loading": {
			"type": "boolean",
			"default": false
		},
		"show_disclaimer": {
			"type": "boolean",
			"default": true
		}
	},
	"example": {
		"attributes": {
			"symptom": "Headache",
			"aiContent": "<h3>Understanding Headaches</h3><p>Headaches are one of the most common health complaints...</p>"
		}
	}
}
