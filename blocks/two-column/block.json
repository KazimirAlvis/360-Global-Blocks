{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "global360blocks/two-column",
	"version": "0.1.0",
	"title": "Two Column Layout",
	"category": "360-blocks",
	"icon": "columns",
	"description": "A full-width two-column layout with image on left and content on right.",
	"attributes": {
		"imageUrl": {
			"type": "string",
			"default": ""
		},
		"imageId": {
			"type": "number",
			"default": 0
		},
		"heading": {
			"type": "string",
			"default": ""
		},
		"bodyText": {
			"type": "string",
			"default": ""
		}
	},
	"example": {},
	"supports": {
		"html": false,
		"align": ["full"]
	},
	"textdomain": "global360blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
