{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "global360blocks/full-hero",
	"title": "Full Page Hero",
	"category": "360-blocks",
	"icon": "admin-page",
	"description": "A large hero block with image, heading, sub-heading, and assessment button.",
	"keywords": ["hero", "banner", "full-width"],
	"textdomain": "global360blocks",
	"attributes": {
		"bgImageUrl": {
			"type": "string",
			"default": ""
		},
		"bgImageId": {
			"type": "number",
			"default": 0
		},
		"heading": {
			"type": "string",
			"default": ""
		},
		"subheading": {
			"type": "string",
			"default": ""
		},
		"assessmentId": {
			"type": "string",
			"default": ""
		}
	},
	"supports": {
		"html": false,
		"align": ["full"],
		"anchor": true,
		"className": true,
		"reusable": true,
		"inserter": true,
		"multiple": true,
		"lock": false
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
